{
  "name": "wasm-clamp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "vite build && touch docs/.nojekyll && git add docs/",
    "preview": "vite preview",
    "fix": "prettier --write .",
    "lint": "",
    "binaryen": "shx cp ./node_modules/binaryen/index.js ./third_party/binaryen.js",
    "deploy": "npm run fix && npm run lint && npm run build && git commit -a && git push",
    "prepare": "npm run binaryen && npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-dynamic-import-vars": "^2.1.0",
    "prettier": "^3.0.3",
    "shx": "^0.3.4",
    "vite": "^4.5.0",
    "vite-plugin-pwa": "^0.16.7"
  },
  "dependencies": {
    "binaryen": "^116.0.0",
    "browser-fs-access": "^0.35.0",
    "pretty-bytes": "^6.1.1"
  }
}
